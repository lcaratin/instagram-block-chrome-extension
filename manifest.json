{
	"manifest_version": 2,
	"name": "Instagram Unlocker",
	"version": "1.0",
	"browser_action": {
		"default_icon": {
			"16": "images/iconfinder_instagram_1107220.png",
			"24": "images/iconfinder_instagram_1107220.png",
			"32": "images/iconfinder_instagram_1107220.png" 
		},
		"default_title": "Instagram Unlocker"
	},
	"content_scripts": [
		{
			"matches": ["http://www.instagram.com/*", "https://www.instagram.com/*"],
			"js": ["./script/controller.js"]
		}
	]
}